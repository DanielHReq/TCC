#:kivy 1.11.0

<Gerenciador>:
    Mapa:
        name:'telaMapa'

<Mapa>:
    ScatterLayout:
        auto_bring_to_front: False
        Image:
            source:'../assets/logo.png'
    FloatLayout:
        BoxLayout:
            pos_hint:{'right':0.9,'y':0.1}
            size_hint_y:None
            size_hint_x:None
            width:200
            height:125
            Image:
                id:imagem
                size:self.texture_size
                allow_stretch:True
                keep_ratio:False
    #            color: (1,0,1,.5)
